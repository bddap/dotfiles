#!/usr/bin/env python3
import csv
import json
import sys

data = json.load(sys.stdin)

if len(data) == 0:
    print("json2csv unable to determine header, no data found", file=sys.stderr)
    sys.exit(0)

writer = csv.DictWriter(sys.stdout, fieldnames=data[0].keys())
writer.writeheader()
for row in data:
    writer.writerow(row)
